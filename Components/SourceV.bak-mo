within HPF.Components;

model SourceV
extends HPF.Interfaces.TwoPinBaseV;
Complex j = Modelica.ComplexMath.j;
//parameter Complex p = 1 + 0*j "Complex Power";
parameter Complex p(re(start = 1), im(start = 0)) "Complex Power, P + jQ = P, Q";

equation
  
  /*
    Modeling a source and a load
  */
  p = v * Modelica.ComplexMath.conj(i);
annotation(
    Icon(coordinateSystem(grid = {0, 0})),
    Diagram(coordinateSystem(extent = {{-200, -200}, {200, 200}}, grid = {0, 0})));
    
end SourceV;